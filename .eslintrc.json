{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["google", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": "off",
    "no-undef": "error",
    "linebreak-style": "off",
    "no-const-assign": "error"
  }
}
